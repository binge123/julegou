<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <RelativeLayout
            android:id="@+id/head"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="52dp"
            android:background="@color/activity_graybg">
            <TextView
                android:textColor="@color/black_text_color"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="个人"
                android:textSize="20sp"/>
        </RelativeLayout>
        <ScrollView
            android:scrollbars="none"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <RelativeLayout
            android:background="#ECECEC"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <RelativeLayout
                android:paddingBottom="15dp"
                android:id="@+id/rl"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="#fff">
                <View
                    android:layout_marginTop="5dp"
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:background="#ECECEC"/>
                <ImageView
                    android:id="@+id/img"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="15dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/img_defined"/>
                <ImageView
                    android:id="@+id/img1"
                    android:layout_marginLeft="10dp"
                    android:layout_centerVertical="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/img"
                    android:background="@drawable/login_btn"/>
                <ImageView
                    android:id="@+id/img2"
                    android:layout_marginLeft="10dp"
                    android:layout_centerVertical="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/img1"
                    android:background="@drawable/regist_btn"/>

            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl1"
                android:layout_below="@id/rl"
                android:background="#fff"
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <RelativeLayout
                    android:id="@+id/rel"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:layout_marginLeft="20dp">
                    <ImageView
                        android:id="@+id/img3"
                        android:layout_centerVertical="true"
                        android:background="@drawable/dingdan"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:layout_marginLeft="10dp"
                        android:text="全部订单"
                        android:textSize="16sp"
                        android:layout_toRightOf="@id/img3"
                        android:layout_centerVertical="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <ImageView
                        android:layout_marginRight="20dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/xuanze1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>
                <View
                    android:id="@+id/view"
                    android:layout_below="@id/rel"
                    android:layout_marginTop="5dp"
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:background="#ECECEC"/>
                <LinearLayout
                    android:layout_below="@id/view"
                    android:background="#FFF"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:id="@+id/daifaline"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:layout_marginTop="20dp"
                        android:layout_marginBottom="20dp"
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/daifa"/>
                        <TextView
                            android:text="待发货"
                            android:layout_marginTop="5dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                    </LinearLayout>
                    <View
                        android:background="#ECECEC"
                        android:layout_width="1dp"
                        android:layout_height="fill_parent"/>
                    <LinearLayout
                        android:id="@+id/daishouline"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:layout_marginTop="20dp"
                        android:layout_marginBottom="20dp"
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/daishou"/>
                        <TextView
                            android:text="待收货"
                            android:layout_marginTop="5dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                    </LinearLayout>
                    <View
                        android:background="#ECECEC"
                        android:layout_width="1dp"
                        android:layout_height="fill_parent"/>
                    <LinearLayout
                        android:id="@+id/daipingline"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:layout_marginTop="20dp"
                        android:layout_marginBottom="20dp"
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/daiping"/>
                        <TextView
                            android:text="待评价"
                            android:layout_marginTop="5dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                    </LinearLayout>
                    <View
                        android:background="#ECECEC"
                        android:layout_width="1dp"
                        android:layout_height="fill_parent"/>
                    <LinearLayout
                        android:gravity="center"
                        android:layout_weight="1"
                        android:layout_marginTop="20dp"
                        android:layout_marginBottom="20dp"
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/tuihuo"/>
                        <TextView
                            android:text="退款"
                            android:layout_marginTop="5dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                    </LinearLayout>
                    <View
                        android:background="#ECECEC"
                        android:layout_width="1dp"
                        android:layout_height="fill_parent"/>
                </LinearLayout>
            </RelativeLayout>
            <LinearLayout
                android:orientation="vertical"
                android:layout_below="@id/rl1"
                android:layout_marginTop="10dp"
                android:background="#fff"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">
                <RelativeLayout
                    android:id="@+id/rel1"
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:layout_marginLeft="20dp">
                    <ImageView
                        android:id="@+id/img5"
                        android:layout_centerVertical="true"
                        android:background="@drawable/people"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:layout_marginLeft="10dp"
                        android:text="个人资料"
                        android:textSize="16sp"
                        android:layout_toRightOf="@id/img5"
                        android:layout_centerVertical="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <ImageView
                        android:layout_marginRight="20dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/xuanze1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>
                <View
                    android:layout_marginRight="10dp"
                    android:layout_marginLeft="10dp"
                    android:background="#ECECEC"
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"/>
                <RelativeLayout
                    android:id="@+id/rel2"
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:layout_marginLeft="20dp">
                    <ImageView
                        android:id="@+id/img6"
                        android:layout_centerVertical="true"
                        android:background="@drawable/myjifen"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:layout_marginLeft="10dp"
                        android:text="我的积分"
                        android:textSize="16sp"
                        android:layout_toRightOf="@id/img6"
                        android:layout_centerVertical="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <ImageView
                        android:layout_marginRight="20dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/xuanze1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>
                <View
                    android:layout_marginRight="10dp"
                    android:layout_marginLeft="10dp"
                    android:background="#ECECEC"
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"/>
                <RelativeLayout
                    android:id="@+id/rel3"
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:layout_marginLeft="20dp">
                    <ImageView
                        android:id="@+id/img7"
                        android:layout_centerVertical="true"
                        android:background="@drawable/mymessages"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:layout_marginLeft="10dp"
                        android:text="我的消息"
                        android:textSize="16sp"
                        android:layout_toRightOf="@id/img7"
                        android:layout_centerVertical="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <ImageView
                        android:layout_marginRight="20dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/xuanze1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>
                <View
                    android:layout_marginRight="10dp"
                    android:layout_marginLeft="10dp"
                    android:background="#ECECEC"
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"/>
                <RelativeLayout
                    android:id="@+id/rel4"
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:layout_marginLeft="20dp">
                    <ImageView
                        android:id="@+id/img8"
                        android:layout_centerVertical="true"
                        android:background="@drawable/zuji"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:layout_marginLeft="10dp"
                        android:text="我的足迹"
                        android:textSize="16sp"
                        android:layout_toRightOf="@id/img8"
                        android:layout_centerVertical="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <ImageView
                        android:layout_marginRight="20dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/xuanze1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>
                <View
                    android:background="#ECECEC"
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"/>
                <RelativeLayout
                    android:id="@+id/rel5"
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:layout_marginLeft="20dp">
                    <ImageView
                        android:id="@+id/img9"
                        android:layout_centerVertical="true"
                        android:background="@drawable/mycollection"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:layout_marginLeft="10dp"
                        android:text="我的关注"
                        android:textSize="16sp"
                        android:layout_toRightOf="@id/img9"
                        android:layout_centerVertical="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <ImageView
                        android:layout_marginRight="20dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/xuanze1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>
                <View
                    android:layout_marginRight="10dp"
                    android:layout_marginLeft="10dp"
                    android:background="#ECECEC"
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"/>
                <RelativeLayout
                    android:id="@+id/rel6"
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:layout_marginLeft="20dp">
                    <ImageView
                        android:id="@+id/img4"
                        android:layout_centerVertical="true"
                        android:background="@drawable/ky"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:layout_marginLeft="10dp"
                        android:text="我的设置"
                        android:textSize="16sp"
                        android:layout_toRightOf="@id/img4"
                        android:layout_centerVertical="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <ImageView
                        android:layout_marginRight="20dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/xuanze1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>
                <View
                    android:layout_marginRight="10dp"
                    android:layout_marginLeft="10dp"
                    android:background="#ECECEC"
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"/>
            </LinearLayout>

        </RelativeLayout>
        </ScrollView>
    </LinearLayout>
</FrameLayout>